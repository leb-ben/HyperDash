# Copy this file to .env and fill in your values

# AI Providers
CEREBRAS_API_KEY=your_cerebras_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Hyperliquid (wallet-based auth)
# You'll need to export your private key from your wallet
# NEVER share this with anyone
HYPERLIQUID_PRIVATE_KEY=your_wallet_private_key_here
HYPERLIQUID_WALLET_ADDRESS=your_wallet_address_here

# Optional: Use testnet first
HYPERLIQUID_TESTNET=true

# Bot Settings
LOG_LEVEL=info
# Trading Configuration
PAPER_TRADING=true
LIVE_TRADING=false
REALISTIC_PAPER_TRADING=true
INITIAL_PAPER_BALANCE=100
